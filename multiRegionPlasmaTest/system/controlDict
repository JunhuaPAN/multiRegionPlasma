FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     multiRegionPlasmaFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         5e-8;

deltaT          2e-10;

writeControl    runTime;

writeInterval   1e-9;

purgeWrite      0;

writeFormat     ascii;

writePrecision  5;

writeCompression on;

timeFormat      general;

timePrecision   5;

runTimeModifiable true;

adjustTimeStep  no;



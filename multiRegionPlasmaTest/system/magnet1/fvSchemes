FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default steadyState;
}

gradSchemes
{
    default       Gauss linear;
}

divSchemes
{
    default       Gauss linear;  
}


laplacianSchemes
{
    default         Gauss linear corrected;
}

curlSchemes
{
    default         Gauss linear;
}

interpolationSchemes
{
    default         linear;
}

